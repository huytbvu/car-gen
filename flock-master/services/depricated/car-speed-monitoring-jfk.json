{
"serviceDescription": {
  "container": {
    "image": "docker://docker.cloud:5000/flock_workloads/car_speed_monitoring",
    "options": ["-n=false", "--privileged",
                "--env=DISCOVERY=etcd_host:vm1.jfk.cloud,service:car_speed_monitoring",
                "--lxc-conf=lxc.network.type = veth",
                "--lxc-conf=lxc.network.link = br0",
                "--lxc-conf=lxc.network.name = eth0",
                "--lxc-conf=lxc.network.flags = up"]
  },
  "id": "car-speed-monitoring-jfk",
  "instances": "1",
  "constraints": [["hostname", "UNIQUE"]],
  "cpus": "1",
  "mem": "4096",
  "uris": [ ],
  "cmd": "dhclient eth0; ifconfig eth0; /home/ericsson/configure.sh; cat /home/ericsson/streamer.cfg; service ssh start; cd /home/ericsson && ./run-spark-app.sh; while [ 1 ]; do sleep 5; done"
  },
"datacenterConstraint": {"id": "jfk"},
"slaDescription": {"policy": "switchproxy"}
}

