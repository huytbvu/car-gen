{
  "container": {
    "image": "docker://docker.cloud:5000/libmesos/ubuntu",
    "options": ["-n=false", 
                "--privileged",
                "--env=DISCOVERY=etcd_host:vm1.jfk.cloud,service_id:lax/sample",
                "--lxc-conf=lxc.network.type = veth",
                "--lxc-conf=lxc.network.link = br0",
                "--lxc-conf=lxc.network.name = eth0",
                "--lxc-conf=lxc.network.flags = up"]
  },
  "id": "lax/sample",
  "instances": "1",
  "globalConstraint": {"datacenterId": "lax"},
  "localConstraint": [["hostname", "UNIQUE"]],
  "slaDescription": {"policy": "switchproxy"},
  "serviceOptions": {"username" : "hello", "password" : "world"},
  "cpus": ".5",
  "mem": "4096",
  "cmd": "dhclient eth0; ifconfig eth0; while [ 1 ]; do sleep 5; done"
}
